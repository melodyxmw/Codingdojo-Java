package com.melody.shapes;


public class Circle extends Dot implements AreaInterface {
	
	private int x;
	private int y;
	private int radius;
	

	@Override
	public double getArea() {
		// TODO Auto-generated method stub
		return radius * radius * Math.PI;
	}

	@Override
	public String getLocation() {
		// TODO Auto-generated method stub
		return null;
	}

}
